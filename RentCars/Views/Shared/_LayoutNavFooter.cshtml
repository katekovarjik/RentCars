@using System.Web.Optimization

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Carbook - Free Bootstrap 4 Template by Colorlib</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800&display=swap" rel="stylesheet">


    @Styles.Render("~/bundles/open-iconic-bootstrap/css")

    @Styles.Render("~/bundles/animate/css")

    @Styles.Render("~/bundles/owl.carousel/css")

    @Styles.Render("~/bundles/owl.theme.default/css")

    @Styles.Render("~/bundles/magnific-popup/css")

    @Styles.Render("~/bundles/aos/css")


    @Styles.Render("~/bundles/ionicons/css")

    @Styles.Render("~/bundles/bootstrap-datepicker/css")

    @Styles.Render("~/bundles/jquery.timepicker/css")

    @Styles.Render("~/bundles/flaticon/css")

    @Styles.Render("~/bundles/icomoon/css")

    @Styles.Render("~/bundles/style/css")


</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        @Html.Partial("_Navigation")
    </nav>

    <div class="content">
        @RenderBody()
    </div>

    <footer class="ftco-footer ftco-bg-dark ftco-section">
        @Html.Partial("_Footer")
    </footer>




    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>


    @Scripts.Render("~/bundles/jquery.min/js")
    @Scripts.Render("~/bundles/jquery-migrate-3.0.1.min/js")
    @Scripts.Render("~/bundles/popper.min/js")
    @Scripts.Render("~/bundles/bootstrap.min/js")
    @Scripts.Render("~/bundles/jquery.easing.1.3/js")
    @Scripts.Render("~/bundles/jquery.waypionts.min/js")
    @Scripts.Render("~/bundles/jquery.stellar.min/js")
    @Scripts.Render("~/bundles/owl.carousel.min/js")
    @Scripts.Render("~/bundles/jquery.magnific-popup.min/js")
    @Scripts.Render("~/bundles/aos/js")
    @Scripts.Render("~/bundles/jquery.animateNumber.min/js")
    @Scripts.Render("~/bundles/bootstrap-datepicker/js")
    @Scripts.Render("~/bundles/jquery.timepicker.min/js")
    @Scripts.Render("~/bundles/scrollax.min/js")
    @Scripts.Render("~/bundles/google-map/js")
    @Scripts.Render("~/bundles/main/js")

    @if (IsSectionDefined("Styles"))
    {
        @RenderSection("Styles", false)
    }






    @RenderSection("scripts", false)


</body>
</html>